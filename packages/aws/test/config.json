{
  "version": 2,
  "parameters": {
    "credentials": {
      "accessKeyId": "Bouzouf",
      "secretAccessKey": "Plop"
    },
    "TEST": "Global",
    "region": "us-east-1",
    "website": ["test.webda.io", "test2.webda.io"],
    "csrfOrigins": ["^accounts\\.google\\.\\w{2,}$", "www\\.facebook\\.com"],
    "locales": ["es-ES", "en-GB", "fr-FR"],
    "sessionSecret": ""
  },
  "module": {
    "services": {
      "Test/AWSEvents": "./moddas/awsevents"
    }
  },
  "services": {
    "ConsoleLogger": {
      "type": "Webda/ConsoleLogger",
      "logLevel": "INFO"
    },
    "ConsoleTestLogger": {
      "type": "Webda/ConsoleLogger",
      "logLevels": "TEST",
      "logLevel": "TEST"
    },
    "SQSQueue": {
      "type": "SQSQueue",
      "endpoint": "http://localhost:4566",
      "queue": "http://localhost:4566/000000000000/webda-test",
      "maxConsumers": 1
    },
    "DebugMailer": {},
    "AWSSecretsManager": {
      "endpoint": "http://localhost:4584"
    },
    "IdentsMapper": {
      "type": "Webda/Mapper",
      "source": "Idents",
      "targetAttribute": "idents",
      "target": "Users",
      "attribute": "_user",
      "fields": "type,_lastUpdate,counter",
      "cascade": true
    },
    "idents": {
      "expose": {
        "restrict": {
          "create": true,
          "update": true,
          "get": true
        }
      },
      "asyncDelete": true,
      "endpoint": "http://localhost:4569",
      "type": "DynamoStore",
      "table": "webda-test-idents",
      "model": "Webda/Ident"
    },
    "users": {
      "expose": {
        "url": "/users",
        "restrict": {
          "authentication": true,
          "create": true
        }
      },
      "scanPage": 2,
      "endpoint": "http://localhost:4569",
      "type": "DynamoStore",
      "table": "webda-test-users",
      "model": "Webda/User"
    },
    "binary": {
      "type": "S3Binary",
      "bucket": "webda-test",
      "endpoint": "http://localhost:4566",
      "forcePathStyle": true,
      "map": {
        "users": ["images"]
      },
      "expose": {
        "url": "/binary"
      }
    },
    "awsEvents": {
      "type": "Test/AWSEvents"
    }
  }
}
